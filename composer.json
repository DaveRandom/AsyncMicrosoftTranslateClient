{
    "name": "daverandom/async-microsoft-translate-client",
    "description": "Asynchronous Microsoft Translation API client built on the amp concurrency framework",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Wright",
            "email": "cw@daverandom.com"
        }
    ],
    "keywords": [
        "microsoft",
        "translate",
        "translation",
        "api",
        "client",
        "async",
        "non-blocking",
        "amp"
